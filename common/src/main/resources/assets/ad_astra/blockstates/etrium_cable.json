{
  "multipart": [
    {
      "apply": {
        "model": "ad_astra:block/etrium_cable_core"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "ad_astra:block/etrium_cable_side"
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "ad_astra:block/etrium_cable_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "ad_astra:block/etrium_cable_side",
        "y": 180
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "ad_astra:block/etrium_cable_side",
        "y": 270
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "ad_astra:block/etrium_cable_top"
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "ad_astra:block/etrium_cable_bottom"
      }
    }
  ]
}